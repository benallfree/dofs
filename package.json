{
  "name": "dofs-root",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "format": "prettier --write ."
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "semi": false,
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "wrangler": "^4.14.1"
  },
  "packageManager": "bun@1.1.13"
}
