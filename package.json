{
  "name": "dofs-root",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "format": "prettier --write ."
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "semi": false,
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "workspaces": [
    "packages/*"
  ]
}
